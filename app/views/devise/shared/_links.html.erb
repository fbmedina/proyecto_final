<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
	  <div class="col-6 col-offset-3">
	    <%= link_to "Sign in with #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: "button facebook" %><br />
	   </div>
  <% end -%>
<% end -%>