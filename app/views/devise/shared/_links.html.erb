<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
	  <div class="col-6 col-offset-3 no-padding">
	    <%= link_to "Entra con #{OmniAuth::Utils.camelize(provider)}", omniauth_authorize_path(resource_name, provider), class: "button facebook full-size" %><br />
	   </div>
  <% end -%>
<% end -%>